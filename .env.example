# Skillsight environment variables
# Copy to .env and fill in values: cp .env.example .env

# --- Pipeline settings ---
# SKILLSIGHT_OUTPUT_DIR=./data
# SKILLSIGHT_CONCURRENCY=20
# SKILLSIGHT_RATE_LIMIT_PER_SECOND=20.0
# SKILLSIGHT_REQUEST_TIMEOUT=30.0

# --- Discovery tuning ---
# SKILLSIGHT_PASSES_MAX=10
# SKILLSIGHT_CONVERGE_REPOS=2
# SKILLSIGHT_CONVERGE_GROWTH=0.1
# SKILLSIGHT_SEARCH_QUERY_LIMIT=1000
# SKILLSIGHT_SEARCH_BATCH_SIZE=50

# --- Extraction ---
# SKILLSIGHT_STRUCTURED_ONLY=true
# SKILLSIGHT_RESUME=true

# --- Browser fallback (optional) ---
# SKILLSIGHT_USE_BROWSER=false
# SKILLSIGHT_BROWSER_HEADLESS=true
# SKILLSIGHT_BROWSER_BLOCK_THRESHOLD_PERCENT=2.0
# SKILLSIGHT_BROWSER_BLOCK_WINDOW=500

# --- R2 export (required for deployed snapshot uploads) ---
# SKILLSIGHT_R2_ENDPOINT_URL=https://<account-id>.r2.cloudflarestorage.com
# SKILLSIGHT_R2_ACCESS_KEY_ID=
# SKILLSIGHT_R2_SECRET_ACCESS_KEY=
# SKILLSIGHT_R2_BUCKET_NAME=skillsight-data
# SKILLSIGHT_R2_PREFIX=snapshots  # keep as `snapshots` for Worker compatibility
# SKILLSIGHT_WEB_EXPORT_PREFIX=data/v1
# SKILLSIGHT_WEB_EXPORT_PAGE_SIZE=12
# SKILLSIGHT_R2_RETAIN_CANONICAL_DAYS=30
# SKILLSIGHT_R2_RETAIN_WEB_DAYS=90

# --- Dataset mirrors (optional) ---
# SKILLSIGHT_GITHUB_RELEASE_ENABLED=false
# SKILLSIGHT_GITHUB_RELEASE_REPO=<owner>/<repo>
# SKILLSIGHT_KAGGLE_PUBLISH_ENABLED=false
# SKILLSIGHT_KAGGLE_DATASET_SLUG=<owner>/<dataset>

# --- Prefect (optional, for remote orchestration) ---
# SKILLSIGHT_PREFECT_API_URL=

# --- Web runtime (set in web/.env for Vite) ---
# VITE_USE_MOCKS=false
# VITE_DATA_BASE_URL=
# VITE_SEARCH_API_BASE_URL=
# VITE_API_BASE_URL=  # legacy fallback (transition only)
